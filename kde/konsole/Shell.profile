[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Solarized Dark
Font=DejaVu Sans Mono for Powerline,9,-1,5,50,0,0,0,0,0

[Cursor Options]
CustomCursorColor=255,255,255
UseCustomCursorColor=false

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/zsh
Environment=TERM=xterm-256color
Icon=utilities-terminal
LocalTabTitleFormat=%d : %n
MenuIndex=1
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%h : %u
TabBarMode=1
TabBarPosition=1

[Interaction Options]
UnderlineLinksEnabled=false

[Keyboard]
KeyBindings=linux

[Scrolling]
HistoryMode=1
HistorySize=10000
ScrollBarPosition=2
